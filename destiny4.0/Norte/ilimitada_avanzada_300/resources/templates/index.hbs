<div class="vfes-ms-content ws10-u-color--bg-light-grey" id="maxi" style="visibility: hidden;opacity: 0">


    <!-- BANNER & DIV TICKET QUE INYECTA WCS -->
    {{> headerTicket }}

    <!-- CONTENIDO EN 2/3 DE LA PÁGINA -->
    <div class="ws10-o-wrapper">
        <div class="ws10-o-layout">
            <div class="ws10-o-layout__item ws10-u--2/3@desktop ws10-u--3/5@tablet">
                <!-- MÓVIL -->
                {{> mobile }}
                <!-- SEGUNDA LÍNEA GRATIS -->
                {{> secondline }}

                <!--TV UNIVERSAL -->
                {{> tv }}
                <!-- DECO -->
                {{> deco }}

                <!-- FIBRA -->
                {{> fibra}}

                <!-- TAMBIÉN INCLUIDO -->
                 {{> also-included }}

                <!-- SIN SORPRESAS -->
                 {{> sinsorpresas }}
                 
                <!-- TIMELINE  -->
                {{> timeLine }}

            </div>
        </div>
    </div>
    <!-- STICKY BOTTOM -->
    <section class="vf-sticky ws10-c-card-rate-sticky ws10-u--hidden" data-js-vf="_stickyRate">
        <div class="ws10-c-card ws10-c-card--no-borders-bottom-radius">
            <div>
                <div class="ws10-c-card-rate-sticky__w-cta" data-js-vf="_portability">
                    <div class="ws10-c-card-rate-sticky__offer ws10-flex ws10-flex__direction--column">
                        <del class="ws10-c-card-rate-sticky__dto-price">56,99€/mes</del>
                        <span class="ws10-c-card-rate-sticky__final-price">
                            <span class="ws10-c-card-rate-sticky__final-price-value">28,49€</span>
                            <span class="ws10-c-card-rate-sticky__final-price-date">/mes</span>
                        </span>
                    </div>
                    <a href="https://www.vodafone.es/c/tienda-online/particulares/contratacion?id=1285772&tcli=0&tta=1"
                        type="submit" name="cta" title="CTA" onmousedown="addtocart(utag_data.plans_add2cart[0])"
                        class="ws10-c-button ws10-c-button--auto" data-analytics-element="stiky inferior"
                        data-analytics-context="lo quiero" data-analytics-category="cta"
                        data-analytics-id="-4725875160585">
                        Lo quiero
                    </a>
                </div>
                <div class="ws10-c-card-rate-sticky__w-cta ws10-u--hidden" data-js-vf="_newNumber">
                    <div class="ws10-c-card-rate-sticky__offer ws10-flex ws10-flex__direction--column">
                        <del class="ws10-c-card-rate-sticky__dto-price">56,99€/mes</del>
                        <span class="ws10-c-card-rate-sticky__final-price">
                            <span class="ws10-c-card-rate-sticky__final-price-value">28,49€</span>
                            <span class="ws10-c-card-rate-sticky__final-price-date">/mes</span>
                        </span>
                    </div>
                    <a href="https://www.vodafone.es/c/tienda-online/particulares/contratacion?id=1285753&tcli=0&tta=1 "
                        type="submit" name="cta" title="CTA" class="ws10-c-button ws10-c-button--auto"
                        data-analytics-element="stiky inferior" data-analytics-context="lo quiero"
                        data-analytics-category="cta" data-analytics-id="-4545656976523"
                        onmousedown="addtocart(utag_data.plans_add2cart[1])">

                        Lo quiero
                    </a>
                </div>
            </div>
        </div>
    </section>

