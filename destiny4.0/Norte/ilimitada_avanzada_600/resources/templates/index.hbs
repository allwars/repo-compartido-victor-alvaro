                class="ws10-c-price ws10-c-price--x-small ws10-c-price--default@tablet ws10-c-price--default@desktop">
                <div data-js-vf="_portability">
                  <span class="ws10-c-price__amount">79,49</span>
                </div>
                <div class="ws10-u--hidden" data-js-vf="_newNumber">
