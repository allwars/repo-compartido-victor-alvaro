<link href="css/ws10.css" rel="stylesheet" />
<link href="css/styles.css" rel="stylesheet" data-ws10-microcss />
<div
  class="vfes-ms-content ws10-u-color--bg-light-grey"
  id="ilimitada"
  style="visibility: hidden;opacity: 0"
>
  {{! Filete cliente }}
  {{> filete-cliente }}
  <!-- BANNER & DIV TICKET QUE INYECTA WCS -->
  {{> banner }}
  <!-- CONTENIDO EN 2/3 DE LA PÁGINA -->
  <div class="ws10-o-wrapper">
    <div class="ws10-o-layout">
      <div class="ws10-o-layout__item ws10-u--2/3@desktop ws10-u--3/5@tablet">
        <!-- MÓVIL -->
        {{> mobile }}
        <!-- SEGUNDA LÍNEA GRATIS -->
        {{> second-line }}
        <!-- FIBRA -->
        {{> fibra }}
        <!-- TAMBIÉN INCLUIDO -->
        {{> also-included }}
        <!-- SIN SORPRESAS -->
        {{> no-surprises }}
        <!-- TIMELINE  -->
        {{> timeline }}
      </div>
    </div>
  </div>
  {{! BACKDROP FILETE CLIENTE }}
  {{> backdrop-filete }}
  <!-- STICKY BOTTOM -->
  {{> sticky-bottom }}
  <!-- TERMINALES-->
  {{> terminals }}
</div>

<script src="js/ws10.js"></script>
<script src="js/main.js"></script>
